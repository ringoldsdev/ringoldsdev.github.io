{
  "scripts": {
    "start": "npm-run-all --parallel css eleventy browsersync",
    "eleventy": "eleventy --watch",
    "debug": "set DEBUG=* & eleventy",
    "css": "postcss src/static/css/tailwind.css --o _site/static/css/style.css --watch",
    "build": "cross-env NODE_ENV=production eleventy && cross-env NODE_ENV=production tailwindcss -i src/static/css/tailwind.css -o _site/static/css/style.css",
    "browsersync": "browser-sync start --server _site --files _site --port 8080 --no-notify --no-open"
  },
  "devDependencies": {
    "@11ty/eleventy": "2.0.1",
    "@11ty/eleventy-plugin-syntaxhighlight": "5.0.0",
    "@tailwindcss/typography": "0.5.9",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "alpinejs": "3.12.3",
    "browser-sync": "2.29.3",
    "cross-env": "7.0.3",
    "cssnano": "6.0.1",
    "esbuild": "^0.18.16",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^37.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "html-minifier": "4.0.0",
    "js-yaml": "4.1.0",
    "luxon": "3.3.0",
    "npm-run-all": "4.1.5",
    "postcss-cli": "9.1.0",
    "prettier": "^3.0.0",
    "prismjs": "1.29.0",
    "tailwindcss": "3.3.3",
    "typescript": "*"
  },
  "dependencies": {
    "autoprefixer": "10.4.14",
    "postcss": "8.4.27"
  }
}